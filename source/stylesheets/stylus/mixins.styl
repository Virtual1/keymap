keyshadow()
  background rgba(255, 255, 255, 0.75)
  -webkit-box-shadow 0 3px 0 0 rgba(0, 0, 0, 0.1)
  -moz-box-shadow 0 3px 0 0 rgba(0, 0, 0, 0.1)
  box-shadow 0 3px 0 0 rgba(0, 0, 0, 0.1)
  color rgba(0, 0, 0, 0.4)
  // @include filter-gradient(#1fffff, #00ffff, vertical); // IE6-9
  // clude background-image(linear-gradient(top,  rgba(255,255,255,0.12) 0%,rgba(255,255,255,0) 30%,rgba(255,255,255,0) 100%));
    
keyshadowhover()
  background rgba(255, 255, 255, 0.75)
  -webkit-box-shadow 0 3px 0 0 rgba(0, 0, 0, 0.1)
  -moz-box-shadow 0 3px 0 0 rgba(0, 0, 0, 0.1)
  box-shadow 0 3px 0 0 rgba(0, 0, 0, 0.1)
  // @include filter-gradient(#1fffff, #00ffff, vertical); // IE6-9
  // clude background-image(linear-gradient(top,  rgba(255,255,255,0.12) 0%,rgba(255,255,255,0) 30%,rgba(255,255,255,0) 100%));
    
keyshadowactive()
  color rgba(255, 255, 255, 0.25)
  background rgba(0, 0, 0, 0.15)
  border 2px solid rgba(0, 0, 0, 0.25)
  -webkit-box-shadow none
  -moz-box-shadow none
  box-shadow none
  
lightkeyshadowhover()
  background rgba(255, 255, 255, 1)
  -webkit-box-shadow 0 3px 0 0 rgba(0, 0, 0, 0.1)
  -moz-box-shadow 0 3px 0 0 rgba(0, 0, 0, 0.1)
  box-shadow 0 3px 0 0 rgba(0, 0, 0, 0.1)
  // @include filter-gradient(#1fffff, #00ffff, vertical); // IE6-9
  // clude background-image(linear-gradient(top,  rgba(255,255,255,0.12) 0%,rgba(255,255,255,0) 30%,rgba(255,255,255,0) 100%));
    
lightkeyshadowactive()
  color rgba(0, 0, 0, 0.2)
  background rgba(255, 255, 255, 0.3)
  border 2px solid rgba(0, 0, 0, 0.17)
  -webkit-box-shadow none
  -moz-box-shadow none
  box-shadow none
  
btn()
  filter-gradient(black, #260000, vertical)
  $experimental-support-for-svg = true
  background-image(linear-gradient(top, rgba(0, 0, 0, 0) 0%, rgba(0, 0, 0, 0.08) 100%))
  -webkit-box-shadow inset 0px 2px 0px 0px rgba(255, 255, 255, 0.08)
  box-shadow inset 0px 2px 0px 0px rgba(255, 255, 255, 0.08)
  
btnhover()
  filter-gradient(black, #260000, vertical)
  $experimental-support-for-svg = true
  background-image(linear-gradient(top, rgba(0, 0, 0, 0) 0%, rgba(0, 0, 0, 0.25) 100%))
  -webkit-box-shadow inset 0px 2px 0px 0px rgba(255, 255, 255, 0.1)
  box-shadow inset 0px 2px 0px 0px rgba(255, 255, 255, 0.1)
  
btnactive()
  -webkit-box-shadow inset 0px 0px 1px 0px rgba(0, 0, 0, 0.45)
  box-shadow inset 0px 0px 1px 0px rgba(0, 0, 0, 0.45)
  
key()
  filter-gradient(black, #0d0000, vertical)
  $experimental-support-for-svg = true
  background-image(linear-gradient(top, rgba(0, 0, 0, 0) 0%, rgba(0, 0, 0, 0.55) 51%, rgba(0, 0, 0, 0.05) 100%))
  
twosecs()
  transition all 0.2s
  -moz-transition all 0.2s
  -webkit-transition all 0.2s
  
onesec()
  transition all 0.05s
  -moz-transition all 0.05s
  -webkit-transition all 0.05s
  
